<Window x:Class="RealEstateDirectory.MainFormTabs.Room.DialogTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DialogTest" Height="300" Width="300">
    <StackPanel>
        <TextBlock >Проверка</TextBlock>
        <TextBlock Grid.Row="0" Grid.Column="0" Text="Район" Margin="0,2,5,2" />
        <ComboBox Grid.Row="0" Grid.Column="1" ItemsSource="{Binding Districts}" SelectedItem="{Binding CurrentDistrict}"/>

        <TextBlock Grid.Row="0" Grid.Column="2" Text="Улица" Margin="5,2,5,2" />
        <ComboBox Grid.Row="0" Grid.Column="3" ItemsSource="{Binding Streets}" SelectedItem="{Binding CurrentStreet}"/>
        <Button Grid.Column="0" Margin="5" Command="{Binding OkCommand}" Width="150" Height="25">Ок</Button>
    </StackPanel>
</Window>
